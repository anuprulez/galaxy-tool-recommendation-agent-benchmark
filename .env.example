# Jetstream OpenAI-compatible base URL
JETSTREAM_API_BASE=https://llm.jetstream-cloud.org/api/

# API key for Jetstream (leave blank if not required)
JETSTREAM_API_KEY=replace-with-key

# Default model ID
JETSTREAM_MODEL=llama-4-scout

# Optional sampling parameters
JETSTREAM_TEMPERATURE=0.5
JETSTREAM_TOP_P=0.9
JETSTREAM_MAX_TOKENS=1024

# Optional dataset size cap (bytes). Tutorials whose datasets exceed this are skipped.
# Example: 1GB = 1073741824
# MAX_DATASET_BYTES=1073741824
